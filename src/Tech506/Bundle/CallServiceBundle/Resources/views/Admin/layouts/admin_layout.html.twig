    <link href="{{ asset('admin/css/bootstrapValidator.min.css') }}" rel="stylesheet" />

    <!-- Custom styling plus plugins -->
    <link href="{{ asset('gentella/css/custom.css') }}" rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="{{ asset('gentella/css/maps/jquery-jvectormap-2.0.1.css') }}" />
    <link href="{{ asset('gentella/css/icheck/flat/green.css') }}" rel="stylesheet" />
    <link href="{{ asset('gentella/css/floatexamples.css') }}" rel="stylesheet" type="text/css" />

    <script src="{{ asset('gentella/js/jquery.min.js') }}"></script>

    <!-- bootstrap datepicker -->
    <link href="{{ asset('admin/assets/bootstrap-datepicker-1.5.1/css/bootstrap-datepicker.css') }}" rel="stylesheet">

    <!-- bootstrap timepicker -->
    <link href="{{ asset('admin/assets/bootstrap-timepicker/css/bootstrap-timepicker.css') }}" rel="stylesheet">

    <!-- select2 -->
    <link href="{{ asset('gentella/css/select/select2.min.css') }}" rel="stylesheet">

    <!--[if lt IE 9]>
