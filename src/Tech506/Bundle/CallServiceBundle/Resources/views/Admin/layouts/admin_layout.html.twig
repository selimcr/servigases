    <![endif]-->

    {% block css %}
    {% endblock %}
</head>


<body class="nav-md">

<div class="container body">
    <div class="main_container">
        <div class="col-md-3 left_col">
            <div class="left_col scroll-view">
                <div class="navbar nav_title" style="border: 0;">
                    <a href="#" class="site_title profile_pic">
                        <img src="{{ asset('favicon.ico') }}" alt="..."><span>{{ 'servigases'|trans }}</span></a>
                </div>
                <div class="clearfix"></div>

                <!-- menu prile quick info -->
                <div class="profile">
                    <div class="profile_pic">
                        <img src="{{ app.user.getAvatarUrl(asset('admin/img/default_avatar_male.jpg'), asset('uploads/avatars/')) }}"
                            class="img-circle profile_img">
                    </div>
                    <div class="profile_info">
                        <span>{{ 'welcome'|trans }},</span>
                        <h2>{{ app.user.name ~ " " ~ app.user.lastname}}</h2>
                    </div>
                </div>
                <!-- /menu prile quick info -->

                <br />

                <!-- sidebar menu -->
                <div id="sidebar-menu" class="main_menu_side hidden-print main_menu">
                    <div class="menu_section">
                        <h3>&nbsp;</h3>
                        <ul class="nav side-menu">
                            <li>
                                <a href="{{ path('_admin_home') }}">
                                    <i class="fa fa-home"></i><span>{{ 'home'|trans }}</span>
                                </a>
                            </li>
                            {{ renderMenu()|raw }}
                        </ul>
                    </div>
                </div>
                <!-- /sidebar menu -->

                <!-- /menu footer buttons -->
                <div class="sidebar-footer hidden-small">
                    <a href="{{ path('_admin_my_account') }}" data-toggle="tooltip" data-placement="top" title="{{ 'my.profile'|trans }}">
                        <span class="glyphicon glyphicon-cog" aria-hidden="true"></span>
                    </a>
                    <a data-toggle="tooltip" data-placement="top" title="FullScreen">
                        <span class="glyphicon glyphicon-fullscreen" aria-hidden="true"></span>
                    </a>
                    <a data-toggle="tooltip" data-placement="top" title="Lock">
                        <span class="glyphicon glyphicon-eye-close" aria-hidden="true"></span>
                    </a>
                    <a href="{{ path('_logout') }}" data-toggle="tooltip" data-placement="top" title="{{ 'log.out'|trans }}">
                        <span class="glyphicon glyphicon-off" aria-hidden="true"></span>
                    </a>
                </div>
                <!-- /menu footer buttons -->
            </div>
        </div>

        <!-- top navigation -->
        <div class="top_nav">

            <div class="nav_menu">
                <nav class="" role="navigation">
                    <div class="nav toggle">
                        <a id="menu_toggle"><i class="fa fa-bars"></i></a>
                    </div>

                    <ul class="nav navbar-nav navbar-right">
                        <li class="">
                            <a href="javascript:;" class="user-profile dropdown-toggle" data-toggle="dropdown" aria-expanded="false">
                                <img src="{{ app.user.getAvatarUrl(asset('admin/img/default_avatar_male.jpg'), asset('uploads/avatars/')) }}"
                                     alt="">{{ app.user.name ~ " " ~ app.user.lastname}}
                                <span class=" fa fa-angle-down"></span>
                            </a>
                            <ul class="dropdown-menu dropdown-usermenu animated fadeInDown pull-right">
                                <li>
                                    <a href="{{ path('_admin_my_account') }}">  {{ 'my.profile'|trans }}</a>
                                </li>
                                <li>
                                    <a href="{{ path('_logout') }}"><i class="fa fa-sign-out pull-right"></i> {{ 'log.out'|trans }}</a>
                                </li>
                            </ul>
                        </li>

                        <li role="presentation" class="dropdown">
                            <a href="javascript:;" class="dropdown-toggle info-number" data-toggle="dropdown" aria-expanded="false">
                                <i class="fa fa-envelope-o"></i>
                                <span class="badge bg-green">6</span>
                            </a>
                            <ul id="menu1" class="dropdown-menu list-unstyled msg_list animated fadeInDown" role="menu">
                                <li>
                                    <a>
                                            <span class="image">
                                        <img src="{{ asset('gentella/images/img.jpg') }}" alt="Profile Image" />
                                    </span>
                                            <span>
                                        <span>John Smith</span>
                                            <span class="time">3 mins ago</span>
                                            </span>
                                            <span class="message">
                                        Film festivals used to be do-or-die moments for movie makers. They were where...
                                    </span>
                                    </a>
                                </li>
                                <li>
                                    <div class="text-center">
                                        <a>
                                            <strong><a href="#">See All Alerts</strong>
                                            <i class="fa fa-angle-right"></i>
                                        </a>
                                    </div>
                                </li>
                            </ul>
                        </li>

                    </ul>
                </nav>
            </div>
        </div>
        <!-- /top navigation -->


        <!-- page content -->
        <div class="right_col" role="main">
            <!-- -->
            {% block mainContent %}
            {% endblock mainContent %}
            <!-- -->
        </div>
        <!-- /page content -->
    </div>

</div>

{% block bodyModals %}
{% endblock bodyModals %}

<div id="custom_notifications" class="custom-notifications dsp_none">
    <ul class="list-unstyled notifications clearfix" data-tabbed_notifications="notif-group">
    </ul>
    <div class="clearfix"></div>
    <div id="notif-group" class="tabbed_notifications"></div>
</div>

<div class="modal" id="pleaseWaitDialog" data-backdrop="static" data-keyboard="false">
    <div class="loader">Loading...</div>
</div>

